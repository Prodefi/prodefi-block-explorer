{
  "name": "prodefi-block-explorer",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "prebuild": "npm run build:clean",
    "postinstall": "npm run build:dll",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "styled-components"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:flowtype/recommended",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "flowtype",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ],
      "no-console": 0,
      "class-methods-use-this": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/require-default-props": 0,
      "react/require-extension": 0,
      "react/no-unused-state": 0,
      "react/prop-types": 0,
      "no-case-declarations": 0,
      "import/no-named-as-default": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "linebreak-style": 0,
      "react/no-array-index-key": 0,
      "react/destructuring-assignment": 0,
      "no-param-reassign": 0,
      "camelcase": 0,
      "no-unused-expressions": "off",
      "react/jsx-no-bind": 0,
      "react/no-unused-prop-types": 0,
      "no-unused-vars": 0,
      "no-nested-ternary": 0,
      "no-shadow": "off",
      "no-unused-state": 0,
      "array-callback-return": "off",
      "prefer-destructuring": "off"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "url-loader",
      "concurrently",
      "cors"
    ],
    "include": [
      "core-js",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "babel-polyfill": "6.26.0",
    "bootstrap": "^4.2.1",
    "bugsnag-js": "^4.7.3",
    "bugsnag-react": "^1.1.1",
    "chalk": "^2.4.1",
    "chart.js": "^2.7.2",
    "compression": "1.7.3",
    "concurrently": "^3.6.1",
    "containers": "0.0.1",
    "cors": "^2.8.4",
    "cross-env": "5.2.0",
    "d3": "^5.7.0",
    "eosjs": "^16.0.0",
    "eosjs-api": "^7.0.3",
    "express": "4.16.3",
    "grid-styled": "^4.2.0",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^3.0.0",
    "history": "4.7.2",
    "i": "^0.3.6",
    "i18next": "^11.5.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jquery": "^3.4.1",
    "lodash": "4.17.19",
    "minimist": "1.2.0",
    "moment": "^2.23.0",
    "moment-range": "^4.0.2",
    "polished": "^1.9.2",
    "prop-types": "15.6.2",
    "query-string": "^6.0.0",
    "react": "^16.13.0",
    "react-bootstrap-typeahead": "^3.4.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-i18next": "^7.10.1",
    "react-js-pagination": "^3.0.3",
    "react-json-view": "^1.19.1",
    "react-loadable": "5.4.0",
    "react-middle-truncate": "^1.0.3",
    "react-number-format": "^4.4.1",
    "react-paginate": "^6.3.2",
    "react-redux": "5.0.7",
    "react-responsive-carousel": "^3.1.41",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-select": "^2.0.0",
    "react-world-flags": "^1.2.4",
    "reactstrap": "^6.5.0",
    "redux": "4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "3.0.1",
    "sanitize.css": "6.0.0",
    "socket.io-client": "^2.1.1",
    "store": "^2.0.12",
    "styled-components": "3.4.0",
    "styled-system": "^3.0.2",
    "tls": "0.0.1",
    "url-loader": "^1.0.1",
    "uuid": "^3.2.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "2.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.14",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "circular-dependency-plugin": "5.0.2",
    "css-loader": "0.28.11",
    "empty-module": "0.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.11.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "lint-staged": "7.3.0",
    "moment-locales-webpack-plugin": "^1.1.2",
    "offline-plugin": "5.0.5",
    "prettier": "^1.14.0",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "4.16.4",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3"
  }
}
